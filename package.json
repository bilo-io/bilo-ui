{
  "name": "bilo-ui",
  "version": "0.1.1",
  "description": "XUI is a set of experimental ui elements and utilities for frontend web development.",
  "main": "dist/index.js",
  "scripts": {
    "deploy": "bash ./scripts/deploy.sh",
    "bump": "node ./scripts/semver.js",
    "release": "npm publish",
    "prepublish": "yarn build",
    "start": "bash ./scripts/dev_and_demo.sh",
    "serve": "node ./artifact/server.js",
    "dev": "webpack -p --colors -w",
    "dev:all": "yarn start",
    "dev:docs": "UI_ENV=development yarn docs-build",
    "docs-build": "webpack-dev-server -w --color --inline --config webpack.docs.config.js",
    "build": "webpack -p",
    "build:all": "yarn clean:all && yarn build && yarn build:docs && yarn build:server",
    "build:docs": "yarn clean:docs && UI_ENV=production webpack --config webpack.docs.config.js && mv ./demo ./docs",
    "build:server": "webpack -p --config webpack.docs.config.js && mkdir artifact && cp server.js ./artifact && cp -r ./demo ./artifact",
    "clean": "rm -rf artifact && rm -rf dist",
    "clean:all": "yarn clean && yarn clean:docs",
    "clean:docs": "rm -rf docs && rm -rf demo",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:output": "jest --json --outputFile=.jest-test-results.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bilo-io/bilo-ui.git"
  },
  "author": "Bilo Lwabona",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bilo-io/bilo-ui/issues"
  },
  "homepage": "https://github.com/bilo-io/bilo-ui#readme",
  "jest": {
    "verbose": true
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-info": "^3.4.7",
    "@storybook/addon-links": "^3.4.7",
    "@storybook/addon-options": "^3.4.7",
    "@storybook/addon-storysource": "^3.4.7",
    "@storybook/addons": "^3.4.7",
    "@storybook/react": "^3.4.7",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "child_process": "^1.0.2",
    "colors": "^1.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "cypress": "^3.0.1",
    "file-loader": "^1.1.5",
    "file-system": "^2.2.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.1.0",
    "jsx-to-string-2": "^1.0.0",
    "markdown-loader": "^3.0.0",
    "node-sass": "^4.5.3",
    "react": "^16.1.0",
    "react-dom": "^16.2.0",
    "react-highlight.js": "^1.0.7",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "readline": "^1.3.0",
    "sass-loader": "^6.0.6",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "storybook-addon-jest": "^0.0.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "express": "^4.16.2",
    "express-history-api-fallback": "^2.2.1",
    "highlight.js": "^9.12.0",
    "leaflet": "^1.2.0",
    "marked": "^0.3.6",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
