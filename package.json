{
  "name": "bilo-ui",
  "version": "0.0.11",
  "description": "XUI is a set of experimental ui elements and utilities for frontend web development.",
  "main": "dist/index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "deploy": "npm unlink && npm build && npm publish",
    "prepublish": "npm run build",
    "start": "bash ./scripts/dev_and_demo.sh",
    "serve": "node ./artifact/server.js",
    "dev": "webpack -p --colors -w",
    "dev:demo": "webpack-dev-server -w --color --inline --progress --config webpack.demo.config.js",
    "dev:all": "yarn start",
    "build": "webpack -p",
    "build:demo": "webpack -p --config webpack.demo.config.js && mkdir artifact && cp server.js ./artifact && cp -r ./demo ./artifact",
    "build:all": "yarn build && yarn build:demo",
    "clean": "rm -rf artifact && rm -rf dist",
    "clean:demo": "rm -rf ./demo",
    "clean:all": "yarn clean && yarn clean:demo",
    "bump": "semver-bump"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bilo-io/bilo-ui.git"
  },
  "author": "Bilo Lwabona",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bilo-io/bilo-ui/issues"
  },
  "homepage": "https://github.com/bilo-io/bilo-ui#readme",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jsx-to-string-2": "^1.0.0",
    "node-sass": "^4.5.3",
    "react": "^16.1.0",
    "react-dom": "^16.2.0",
    "react-highlight.js": "^1.0.7",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "express": "^4.16.2",
    "express-history-api-fallback": "^2.2.1",
    "highlight.js": "^9.12.0",
    "leaflet": "^1.2.0",
    "marked": "^0.3.6",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
